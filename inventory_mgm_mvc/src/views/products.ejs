        <h1 class="mt-5 mb-4 header">Products</h1>
        <table class="table table-dark">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Name</th>
                    <th scope="col">Description</th>
                    <th scope="col">Price</th>
                    <th scope="col">Image</th>
                    <th scope="col">Action</th>
                </tr>
            </thead>
            <tbody>
                <% products.map(product=>{ %>
                <tr>
                    <th><%= product.id  %></th>
                    <td><%= product.name %></td>
                    <td><%= product.desc %></td>
                    <td scope="col"><%= product.price %></td>
                    <td>
                        <img src="<%= product.imageUrl %>"  style="max-width: 100px;">
                    </td>
                    <td> <a href="/viewProduct/<%= product.id  %>"><button class="btn btn-warning">Update</button></a> 
                        <button class="btn btn-danger" onclick="deleteProduct('<%= product.id %>')" >Delete</button>
                    <!-- <a href="/delete-Product/<%= product.id  %>"><button class="btn btn-danger">Delete</button></a> </td> -->
                </tr>
               <% }) %>
            </tbody>
        </table>
